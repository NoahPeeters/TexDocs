{
  "name": "TexDocsServer",
  "version": "0.0.2",
  "description": "Edit tex documents collaboratively",
  "main": "src/main.js",
  "scripts": {
    "start": "nodemon src/main.js --exec babel-node --presets env -- $NODE_DEBUG_OPTION",
    "build": "babel src -d dist --preset env,stage-2",
    "serve": "node dist/main.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "nodegit": "^0.20.3",
    "promisify-node": "^0.4.0",
    "uuid": "^3.1.0",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "nodemon": "^1.12.1"
  }
}
